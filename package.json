{
  "name": "greekumipsum-cli",
  "version": "1.0.8",
  "preferGlobal": true,
  "description": "Generate random greek paragraphs, words, sentences and characters. Uses the Greekum Ipsum script by Krzysztof Szewczyk.",
  "homepage": "https://krzysztofszewczyk.github.io/MiscStuff/greekum/",
  "repository": "https://github.com/Greekum/greekumipsum",
  "bugs": {
    "url": "https://github.com/Greekum/greekumipsum/issues"
  },
  "main": "cli.js",
  "bin": {
    "greekum": "cli.js"
  },
  "scripts": {
    "test": "xo && ava"
  },
  "keywords": [
    "greekum",
    "ipsum",
    "lorem-ipsum",
    "lorem",
    "greek",
    "cli",
    "generator"
  ],
  "author": "Antoni Kępiński <a@kepinski.me> (https://kepinski.me)",
  "license": "MIT",
  "devDependencies": {
    "ava": "*",
    "execa": "*",
    "xo": "*"
  },
  "xo": {
    "rules": {
      "import/no-unassigned-import": 0,
      "new-cap": 0
    },
    "globals": "GreekumIpsum",
    "ignores": "bin/*.js"
  }
}
